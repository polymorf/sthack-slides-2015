# test14: cheat sheet
msc {
  a, b;

  a -> b   [ label="a -> b  (signal)" ];
  a => b   [ label="a => b  (method)" ];
  b >> a   [ label="b >> a  (return value)" ];
  a =>> b  [ label="a =>> b (callback)" ];
  a -x b   [ label="a -x b  (lost)" ];
  a :> b   [ label="a :> b  (emphasis)" ];
  a .. b   [ label="a .. b  (dotted)" ];
  a note a [ label="a note a\n(note)" ],
  b box b  [ label="b box b\n(action)" ];
  a rbox a [ label="a rbox a\n(reference)" ],
  b abox b [ label="b abox b\n(state/ condition)" ];
  |||      [ label="||| (empty row)" ];
  ...      [ label="... (omitted row)" ];
  ---      [ label="--- (comment)" ];
}