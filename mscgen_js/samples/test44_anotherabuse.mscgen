/*
 * ... and another chart abusing the boundaries
 * of what is possible in mscgen
 */
msc {
  arcgradient=100;
  a [label="", linecolor="white"],
  b [label="", linecolor="white", arclinecolor="blue"],
  c [label="", linecolor="white", arclinecolor="white"],
  d [label="", linecolor="white", arclinecolor="red"],
  e [label="", linecolor="white"];
  
  d -> *[arcskip="-1"],
  b -> *[arcskip="1"],
  ||| [label="mscgen_js\nturns text into sequence charts"];
  |||;
}